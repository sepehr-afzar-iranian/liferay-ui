{
	"packages": {
		"/": {
			"plugins": [],
			".babelrc": {
				"presets": []
			},
			"post-plugins": []
		},
		"*": {
			"plugins": [],
			".babelrc": {
				"presets": []
			},
			"post-plugins": []
		}
	},
	"no-tracking": true,
	"verbose": true,
	"dump-report": false,
	"output": "lib",
	"sources": [
		"src"
	],
	"exclude": {
		"*": true
	},
	"rules": [
		{
			"test": "\\.scss$",
			"exclude": "node_modules",
			"use": [
				"sass-loader"
			]
		},
		{
			"test": "\\.css$",
			"exclude": "node_modules",
			"use": [
				"copy-loader"
			]
		}
	]
}